@{
    var baseurl = Url.Content("~/");
}
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Applications Inventory</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Themes/AdminLte/plugins/fontawesome-free/css/all.min.css">
    <!-- Tempusdominus Bbootstrap 4 -->
    <link rel="stylesheet" href="~/Themes/AdminLte/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="~/Themes/AdminLte/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- JQVMap -->
    <link href="~/Themes/AdminLte/plugins/jqvmap/jqvmap.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Themes/AdminLte/dist/css/adminlte.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="~/Themes/AdminLte/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="~/Themes/AdminLte/plugins/daterangepicker/daterangepicker.css">
    <!-- summernote -->
    <link rel="stylesheet" href="~/Themes/AdminLte/plugins/summernote/summernote-bs4.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <link href="~/Themes/AdminLTE/plugins/sweetalert2/sweetalert2.min.css" rel="stylesheet" />
    <script>
        window.baseUrl = "@baseurl";
    </script>

</head>

<body ng-app="myApp" class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-light navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="~/Home" class="nav-link">
                        Home
                    </a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="~/Home/Support" class="nav-link">Support</a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="~/Home/Project" class="nav-link">Project</a>
                </li>

            </ul>

        </nav>

        <aside class="main-sidebar sidebar-danger elevation-4">
            <!-- Brand Logo -->
            <a href="index3.html" class="brand-link">
                <img src="~/Themes/AdminLte/dist/img/pttlogo.jpg" alt="Ptt Logo" class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-dark">Menu</span>
            </a>
            <div class="sidebar">
                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item has-treeview">
                            
                            
                        </li>

                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>

        </aside>

        <div class="content-wrapper" style="background: white">
            <section class="content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </section>
        </div>
        <footer class="main-footer">
            <strong>Copyright &copy; 2014-2019 <a href="http://adminlte.io">AdminLTE.io</a>.</strong>
            All rights reserved.
            <div class="float-right d-none d-sm-inline-block">
                <b>Version</b> 3.0.5
            </div>
        </footer>

    </div>
    <!-- ./wrapper -->
    <script src="~/js/angularjs/angular.min.js"></script>
    <!-- jQuery -->
    <script src="~/Themes/AdminLte/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/Themes/AdminLte/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Themes/AdminLte/dist/js/adminlte.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/Themes/AdminLte/dist/js/demo.js"></script>
    <script src="~/Themes/AdminLTE/plugins/sweetalert2/sweetalert2.js"></script>

    <script src="~/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/app/app.js"></script>
    @RenderSection(name: "Scripts", required: false)

</body>
</html>